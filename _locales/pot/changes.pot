#: ../../flask/CHANGES.rst:194
msgid "``attachment_filename`` is renamed to ``download_name``."
msgstr ""

#: ../../flask/CHANGES.rst:195
msgid "``cache_timeout`` is renamed to ``max_age``."
msgstr ""

#: ../../flask/CHANGES.rst:196
msgid "``add_etags`` is renamed to ``etag``."
msgstr ""

#: ../../flask/CHANGES.rst:197
msgid "``filename`` is renamed to ``path``."
msgstr ""

#: ../../flask/CHANGES.rst:199
msgid "The ``RequestContext.g`` property is deprecated. Use ``g`` directly or ``AppContext.g`` instead. :issue:`3898`"
msgstr ""

#: ../../flask/CHANGES.rst:201
msgid "``copy_current_request_context`` can decorate async functions. :pr:`4303`"
